{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "iccd"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "bower_components/webcomponentsjs/custom*.js",
        "bower_components/webcomponentsjs/web*.js"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss",
        "../node_modules/ng2-toastr/bundles/ng2-toastr.min.css",
        "bootstrap.min.css",
        "../node_modules/grapesjs/dist/css/grapes.min.css",
        "../node_modules/grapesjs-preset-webpage/dist/grapesjs-preset-webpage.min.css"
      ],
      "scripts": [
        "../bower_components/moment/moment.js",
        "../bower_components/moment-timezone/moment-timezone.js",
        "../node_modules/jquery/dist/jquery.js",
        "../node_modules/grapesjs/dist/grapes.js",
        "../node_modules/grapesjs-style-gradient/dist/grapesjs-style-gradient.min.js",
        "../node_modules/grapesjs-preset-webpage/dist/grapesjs-preset-webpage.min.js",
        "../node_modules/grapesjs-blocks-flexbox/dist/grapesjs-blocks-flexbox.min.js",
        "../node_modules/grapesjs-lory-slider/dist/grapesjs-lory-slider.min.js",
        "../node_modules/grapesjs-tabs/dist/grapesjs-tabs.min.js",
        "../node_modules/grapesjs-style-gradient/dist/grapesjs-style-gradient.min.js",
        "../node_modules/html2canvas/dist/html2canvas.js",
        "../node_modules/simple-drawing-board/dist/simple-drawing-board.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {
    }
  }
}
